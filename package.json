{
  "name": "news_analyzer_frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "start-server": "nodemon --watch 'proxy_server/src/**/*.ts' --exec 'tsc -p tsconfig.proxyserver.json && node proxy_server/dist/server.js'",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
